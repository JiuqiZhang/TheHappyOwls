{
  "expo": {
    "plugins": [
      "expo-apple-authentication",
      "expo-build-properties",
      "expo-secure-store"
    ],
    "name": "TPSI",
    "slug": "tpsi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFD029"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theHappyOwls.Tpsi",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "App would like to access user location to provide nearby happy hour infos, distance from all stores, and map navigations to nearby stores."
      },
      "buildNumber": "1.55",
      "config": {
        "googleMapsApiKey": "AIzaSyBWQ5iFtnT2EQSo1Yi4D6pc-edq7MJkaPA"
      },
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.theHappyOwls.Tpsi"
    },
    "web": {
      "favicon": "./assets/app.png"
    },
    "extra": {
      "eas": {
        "projectId": "40d7be24-514b-46dd-9855-93c6b6e5e274"
      }
    },
    "owner": "9ussel7hang"
  }
}
